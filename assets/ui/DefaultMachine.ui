{
  "type": "Machines:DefaultMachineWindow",
  "skin": "Machines:DefaultMachine",
  "contents": {
    "type": "RowLayout",
    "contents": [
    {
      "type": "RelativeLayout",
      "contents": [
      {
        "type": "ColumnLayout",
        "layoutInfo": {
            "use-content-width": true,
            "use-content-height": true,
            "verticalSpacing": 20,
            "position-vertical-center": {
            },
            "position-horizontal-center": {
            }
          },
          "contents": [
              {
                "type": "FlowLayout",
                "contents": [
                  {
                    "type": "ColumnLayout",
                    "fillVerticalSpace": false,
                    "autoSizeColumns": true,
                    "contents": [
                      {
                        "type": "UILabel",
                        "text": "Materials",
                        "id": "ingredientsInventoryLabel"
                      },
                      {
                        "type": "InventoryGrid",
                        "maxHorizontalCells": 2,
                        "id": "ingredientsInventory"
                      }
                    ]
                  },
                  {
                    "type": "ColumnLayout",
                    "fillVerticalSpace": false,
                    "autoSizeColumns": true,
                    "contents": [
                      {
                        "type": "UILabel",
                        "text": "Tools",
                        "id": "toolsInventoryLabel"
                      },
                      {
                        "type": "InventoryGrid",
                        "maxHorizontalCells": 2,
                        "id": "toolsInventory"
                      }
                    ]
                  },
                  {
                      "type": "HorizontalProgressBar",
                      "width": 100,
                      "id": "progressBar"
                  },
                  {
                    "type": "ColumnLayout",
                    "fillVerticalSpace": false,
                    "autoSizeColumns": true,
                    "contents": [
                      {
                        "type": "UILabel",
                        "text": "Result",
                        "id": "resultInventoryLabel"
                      },
                      {
                        "type": "InventoryGrid",
                        "maxHorizontalCells": 2,
                        "id": "resultInventory"
                      }
                    ]
                  }
                ]
              },
              {
                  "type": "ColumnLayout",
                  "fillVerticalSpace": false,
                  "autoSizeColumns": true,
                  "contents": [
                    {
                      "type": "UIBox",
                      "id": "processResult"
                    },
                    {
                      "type": "UIButton",
                      "text": "Execute",
                      "id": "executeButton"
                    }
                  ]
              },
              {
                "type": "InventoryGrid",
                "id": "playerInventory"
              }
          ]
      }
      ]
    },
    {
        "type": "ScrollableArea",
        "layoutInfo": {
          "useContentWidth": true
        },
        "content": {
              "type": "ProcessListWidget",
              "id": "processList",
              "family": "processlist"
        }
    }
    ]
  }
}
